<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Module>
  <ModulePrefs title="汎用RSSリーダー" description="お好みのRSSを登録して表示します。">
    <Require feature="opensocial-0.8"/> 
    <Require feature="dynamic-height"/> 
 <Require feature="views"/> 
<!--
    <Optional feature="content-rewrite">
			<Param name="include-urls"></Param>
			<Param name="exclude-urls">.*</Param>
			<Param name="include-tags"></Param>
	</Optional>
-->
  </ModulePrefs>
  <Content type="html" view="profile,home,canvas">
  <![CDATA[
<link rel="stylesheet" type="text/css" href="http://test.ngsdev.org/grss/genericrss.css?rnd=3" />
<script type="text/javascript" src="http://jqueryjs.googlecode.com/files/jquery-1.3.1.min.js?rnd=3"></script>
<script type="text/javascript" src="http://test.ngsdev.org/jopensocial/src/jopensocial.js?rnd=3"></script>
<script type="text/javascript" src="http://test.ngsdev.org/grss/genericrss.js?rnd=3"></script>
<div id="wrapper">
  <div id="loading">読み込み中</div>
  <div id="pref" class="view">
    <h2>フィードを管理</h2>
    <p class="add-feed caption"><span class="link">追加</span></p>
    <ul class="list feeds">
    </ul>
    <form onsubmit="return false;">
      <button id="submit-feeds-button">登録</button>
      <button id="cancel-feeds-button">キャンセル</button>
    </form>
  </div>
  <div id="list" class="view">
    <p class="go-pref caption hiddden"><span class="link">フィードを管理</span></p> 
    <ul class="list entries">
    </ul>
  </div>
</div>

]]> </Content>
</Module>

